import java.util.Scanner;

public class sumDigits {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int number = Integer.parseInt(input.nextLine());
        int sum = 0;
        while (number > 0){
            int digit = 0;
            digit = number % 10;
            number /= 10;
            sum += digit;
        }
        System.out.println(sum);
    }
}
